{
    "name": "kenny1911/populate",
    "description": "Library for automatic fill object properties from array or other object.",
    "keywords": ["php", "populate", "fill", "object", "array"],
    "homepage": "https://github.com/Kenny1911/populate",
    "authors": [
        {
            "name": "Kenny1911",
            "email": "o-muzyka@mail.ru"
        }
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2",
        "symfony/property-access": "^4.0.0|^5.0.0"
    },
    "suggest": {
        "symfony/property-access": "Required for using Kenny1911\\Populate\\PropertyAccessor\\SymfonyPropertyAccessor"
    },
    "autoload": {
        "psr-4": {
            "Kenny1911\\Populate\\": "./src/"
        },
        "files": [
            "./src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Kenny1911\\Populate\\Tests\\": "./tests/"
        }
    },
    "scripts": {
        "phpunit": "phpunit"
    }
}
